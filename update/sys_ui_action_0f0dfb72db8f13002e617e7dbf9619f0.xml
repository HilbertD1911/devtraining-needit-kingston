        <script><![CDATA[function pricing(){
	var dialog = new GlideDialogWindow('x_kpmgh_audit_tran_pricing_confirmation');
	dialog.setTitle('Next Step: Pricing');
	dialog.render();
}

if (typeof window == 'undefined') {
	current.workflow_stage = '1';
	//Updating pricing information from prior year
	var gr = new GlideRecord('x_kpmgh_audit_tran_engagement');
	gr.addQuery('company',current.company);
	gr.addQuery('sys_id','!=',current.sys_id);
	gr.orderByDesc('year_end');
	gr.query();
	if(gr.next()) {
		current.fee = gr.fee;
		current.frr = gr.frr;
		current.gross_profit = gr.gross_profit;
		gs.addInfoMessage('Pricing information received from prior year engagement record. Please review and update.');
	}
	//Update and redirect to currnet
	current.update();
	action.setRedirectURL(current);
}]]></script>
